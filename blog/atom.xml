---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Tim Kellogg</title>
 <link href="https://timkellogg.me/blog/atom.xml" rel="self"/>
 <link href="https://timkellogg.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://timkellogg.me/</id>
 <author>
   <name>Tim Kellogg</name>
   <email>timothy.kellogg@gmail.com</email>
 </author>
 <icon>/favicon.ico</icon>

 {% for post in site.posts %}
 {% unless post.is_draft %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="https://timkellogg.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://timkellogg.me{{ post.id }}</id>
   {% if post.summary %}
   <summary type="text">{{ post.summary | strip_html | xml_escape }}</summary>
   {% else %}
   <summary type="text">{{ post.excerpt | strip_html | xml_escape }}</summary>
   {% endif %}
   <content type="text">{{ post.content | strip_html | xml_escape }}</content>
 </entry>
 {% endunless %}
 {% endfor %}
 
</feed>
